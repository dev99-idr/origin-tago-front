							<script>
								
								// 쿠키 가져오기 함수
								function getCookieN(cName) {
									cName = cName + '=';
									var cookieData = document.cookie;
									var start = cookieData.indexOf(cName);
									var cValue = '';
									if(start != -1){
										start += cName.length;
										var end = cookieData.indexOf(';', start);
										if(end == -1)end = cookieData.length;
										cValue = cookieData.substring(start, end);
									}
									return unescape(cValue);
								}

								// locale변경에 따라 class 및 style 변경
								function changeLocale()
								{
									let locale_en = document.getElementById("locale_en");
									let locale_ko = document.getElementById("locale_ko");
									let locale_settings = getCookieN('lang');

									if ( locale_settings == "" ||locale_settings == undefined || locale_settings == null || locale_settings == 'en'){
										locale_en.setAttribute("class","btn btn-rounded btn-success btn-sm") ;
										locale_en.setAttribute("style","color: white;") ;
										locale_ko.setAttribute("class","btn") ;
										locale_ko.setAttribute("style","color: black;") ;
									}else{
										locale_en.setAttribute("class","btn") ;
										locale_en.setAttribute("style","color: black;") ;
										locale_ko.setAttribute("class","btn btn-rounded btn-success btn-sm") ;
										locale_ko.setAttribute("style","color: white;") ;
									}

								}
							</script>

